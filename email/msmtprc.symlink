# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# awn
account        awn
host           mail.andrewalker.net
port           587
from           andre@andrewalker.net
user           andre@andrewalker.net
passwordeval   "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.awn.gpg"

# Set a default account
account default : awn
